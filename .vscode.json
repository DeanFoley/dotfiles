{
    "workbench.startupEditor": "none",
    "terminal.integrated.fontFamily": "'MesloLGS NF'",
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "explorer.confirmDelete": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "editor.fontFamily": "Monaspace Argon, monospace",
    "editor.fontSize": 16,
    "editor.fontWeight": "500",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.defaultProfile.osx": "tmux-shell",
    "terminal.integrated.profiles.osx": {
      "tmux-shell": {
        "path": "tmux",
        "args": ["new-session", "-A", "-s", "${workspaceFolderBasename}-dev"]
      }
    },
    "editor.fontLigatures": true,
    "workbench.colorTheme": "Allogenes",
    "workbench.preferredLightColorTheme": "Allogenes",
    "workbench.preferredDarkColorTheme": "Serika Dark",
    "workbench.preferredHighContrastColorTheme": "Default High Contrast",
    "workbench.preferredHighContrastLightColorTheme": "Default High Contrast Light",
    "[go]": {
      "editor.fontFamily": "'GoMono Nerd Font Mono', monospace"
    },
    "[mdx]": {
      "editor.inlineSuggest.enabled": false,
    },
    "powermode.shake.enabled": false,
    "workbench.fontAliasing": "antialiased",
    "window.autoDetectHighContrast": false,
    "window.autoDetectColorScheme": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.wordWrap": "on",
    "go.delveConfig": {
      "debugAdapter": "dlv-dap",
    },
    "go.toolsEnvVars": {},
    "todohighlight.isEnable": true,
    "todohighlight.include": [
      "**/*.js",
      "**/*.jsx",
      "**/*.ts",
      "**/*.tsx",
      "**/*.html",
      "**/*.css",
      "**/*.scss",
      "**/*.php",
      "**/*.rb",
      "**/*.txt",
      "**/*.mdown",
      "**/*.md",
      "**.*.go"
    ],
    "go.toolsManagement.autoUpdate": true,
    "git.suggestSmartCommit": false,
    "gitlens.rebaseEditor.ordering": "asc",
    "security.workspace.trust.untrustedFiles": "open",
    "update.showReleaseNotes": false,
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "fontRandomizer.fontList": [
      "Monaspace Neon",
      "Monaspace Argon",
      "Monaspace Krypton",
      "Monaspace Xenon"
    ],
    "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.guides.bracketPairs": true,
    "indentRainbow.colorOnWhiteSpaceOnly": true,
    "indentRainbow.indicatorStyle": "light",
    "indentRainbow.lightIndicatorStyleLineWidth": 5,
    "telemetry.telemetryLevel": "off",
    "gitlens.views.branches.branches.layout": "list",
    "eslint.useFlatConfig": true,
    "eslint.format.enable": true,
		"typescript.tsserver.experimental.enableProjectDiagnostics": true,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.telemetry.enabled": false,
    "docker-explorer.enableTelemetry": false,
    "gitlens.views.commits.avatars": false,
    "workbench.colorCustomizations": {
    
    },
    "rubyLsp.enableExperimentalFeatures": true,
    "rubyLsp.rubyVersionManager": {
      "identifier": "asdf"
    },
    "extensions.ignoreRecommendations": true,
    "search.useIgnoreFiles": true,
    "search.useGlobalIgnoreFiles": true,
    "terminal.external.osxExec": "iTerm.app",
    "workbench.settings.editor": "json"
  }